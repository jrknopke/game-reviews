<h1> Edit <%= @review.title %> </h1>

<%= render partial: "layouts/errors", locals: {object: @review} %>

<%= form_for(@review) do |f| %>

    <%= f.label :title %>
    <%= f.text_field :title %>
    <br>
    <br>
    <%= f.label :content %>
    <%= f.text_area :content %>
    <br>
    <%= f.submit %>

<% end %>